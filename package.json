{
  "name": "hustle-finder-ia",
  "version": "3.0.1",
  "type": "module",
  "main": "index-ultra-minimal.js",
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "start": "node index-ultra-minimal.js",
    "dev": "node index-ultra-minimal.js",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev",
    "build": "npm run install-frontend && cd frontend && npm run build",
    "build:railway": "cd frontend && npm ci && npm run build",
    "install-frontend": "cd frontend && npm install",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "cd backend && npm test && cd ../frontend && npm test",
    "lint": "eslint . --ext .js,.mjs,.cjs",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "syntax": "node backend/tools/check-syntax.cjs",
    "audit:static": "node backend/tools/auditStaticDeep.cjs",
    "audit:providers": "node backend/tools/auditProviders.cjs",
    "guard:test": "node backend/tools/testAntiFakeGuard.cjs"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.5.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "vite": "^5.4.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "nodemon": "^3.0.2"
  }
}
