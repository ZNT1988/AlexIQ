# HustleFinderIA Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:5000
VITE_API_TIMEOUT=10000

# Authentication (Clerk)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Environment
VITE_NODE_ENV=development

# Analytics & Monitoring
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# AI Configuration
VITE_AI_API_URL=http://localhost:5000/api/ai
VITE_AI_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_QUANTUM_GENERATOR=true
VITE_ENABLE_VISUAL_CORTEX=true
VITE_DEBUG_MODE=false

# Application Settings
VITE_APP_NAME="HustleFinder IA"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Générateur d'idées business alimenté par l'IA"