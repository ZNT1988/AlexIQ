{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --no-audit --no-fund --legacy-peer-deps --no-workspaces",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}