{
  "name": "alex-ai-backend",
  "version": "1.0.0",
  "description": "Alex AI Backend - Consciousness Engine",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/pre-start-check.js && node index.js",
    "start:safe": "node scripts/pre-start-check.js && node index.js",
    "start:force": "node index.js",
    "start:cluster": "node scripts/pre-start-check.js && node server-cluster.js",
    "start:production": "NODE_ENV=production node server-cluster.js",
    "dev": "node scripts/pre-start-check.js && nodemon index.js",
    "dev:force": "nodemon index.js",
    "dev:cluster": "nodemon server-cluster.js",
    "check": "node scripts/pre-start-check.js",
    "health": "curl -s http://localhost:8080/api/health/detailed || echo 'Backend non accessible'",
    "cluster:status": "curl -s http://localhost:9999/cluster/status || echo 'Cluster status not available'",
    "cluster:health": "curl -s http://localhost:9999/cluster/health || echo 'Cluster health not available'",
    "test": "node test/runner.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:load": "node test/load-test.js",
    "db:migrate": "node db/migrate.js",
    "db:seed": "node db/seed.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@clerk/express": "^1.0.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.1.0",
    "helmet": "^7.1.0",
    "helmet-csp": "^4.0.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^5.6.1",
    "sinon-chai": "^4.0.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@eslint/js": "^9.32.0",
    "chai": "^5.2.1",
    "eslint": "^9.32.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "esmock": "^2.7.1",
    "jest": "^30.0.5",
    "mocha": "^11.7.1",
    "nodemon": "^3.0.2",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.3"
  },
  "keywords": [
    "express",
    "backend",
    "ai",
    "trading",
    "alexai"
  ],
  "author": "AlexAI Team",
  "license": "MIT"
}
