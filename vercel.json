{
  "version": 2,
  "name": "hustlefinder-alexiq",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.alexiq.site/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://api.alexiq.site",
    "VITE_FRONTEND_URL": "https://www.alexiq.site",
    "VITE_NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://www.alexiq.site"
        }
      ]
    }
  ]
}